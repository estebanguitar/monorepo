{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/myapp/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/myapp/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/myapp",
  "projects": {
    "myapp": {
      "type": "application",
      "root": "apps/myapp",
      "entryFile": "main",
      "sourceRoot": "apps/myapp/src",
      "compilerOptions": {
        "tsConfigPath": "apps/myapp/tsconfig.app.json"
      }
    },
    "store": {
      "type": "application",
      "root": "apps/store",
      "entryFile": "main",
      "sourceRoot": "apps/store/src",
      "compilerOptions": {
        "tsConfigPath": "apps/store/tsconfig.app.json"
      }
    },
    "order": {
      "type": "application",
      "root": "apps/order",
      "entryFile": "main",
      "sourceRoot": "apps/order/src",
      "compilerOptions": {
        "tsConfigPath": "apps/order/tsconfig.app.json"
      }
    },
    "deliver": {
      "type": "application",
      "root": "apps/deliver",
      "entryFile": "main",
      "sourceRoot": "apps/deliver/src",
      "compilerOptions": {
        "tsConfigPath": "apps/deliver/tsconfig.app.json"
      }
    }
  }
}